<?xml version="1.0"?>
<!DOCTYPE UWSimScene SYSTEM "UWSimScene.dtd">
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="girona500">

<xacro:include filename="params_base.xacro" />
<xacro:include filename="common.xacro" />


<UWSimScene>
  <oceanState>
    <windx> 0.04 </windx>
    <windy> 0.04 </windy>
    <windSpeed> 12 </windSpeed>
    <depth> 1000 </depth>
    <reflectionDamping> 0.35 </reflectionDamping>
    <waveScale> 1e-7 </waveScale>
    <isNotChoppy> 0 </isNotChoppy>
    <choppyFactor> 2.5 </choppyFactor>
    <crestFoamHeight> 2.2 </crestFoamHeight>
    <oceanSurfaceHeight> 0 </oceanSurfaceHeight>
    <fog>
      <density> 0.1</density>
      <color>
	<r>0</r>
	<g>0.05</g>
	<b>0.3</b>
      </color>
    </fog>
    <color>
      <r>0.0</r>
      <g>0.05</g>
      <b>0.3</b>
    </color>
    <attenuation>
      <r>0.015</r>
      <g>0.0075 </g>
      <b> 0.005 </b>
    </attenuation>
  </oceanState>
  <simParams>
    <disableShaders> 0 </disableShaders>
    <resw> 800 </resw>
    <resh> 600 </resh>
    <offsetp>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </offsetp>
    <offsetr>
      <x> 3.14</x>
      <y> 0</y>
      <z> -1.57 </z>
    </offsetr>
    <enablePhysics> 1 </enablePhysics>
    <showTrajectory>
      <target>girona500</target>
    </showTrajectory>
  </simParams>
  <camera>
    <freeMotion> 1 </freeMotion>
    <objectToTrack>girona500/base_link</objectToTrack>
    <fov> 60 </fov>
    <aspectRatio> 1.33 </aspectRatio>
    <near> 1 </near>
    <far> 10000 </far>
    <position>
      <x>-5</x>
      <y>-5 </y>
      <z>8 </z>
    </position>
    <lookAt>
      <x>0</x>
      <y>0 </y>
      <z>0 </z>
    </lookAt>
  </camera>

  <vehicle>
    <name>girona500</name>
    <file>data/scenes/g500ARM5.urdf</file>
    <jointValues>
      <joint> 0 </joint>
      <joint> 0</joint>
      <joint> 2.2 </joint>
      <joint> 0 </joint>
      <joint>0.3 </joint>
    </jointValues>
    <position>
      <x> 0.2</x>
      <y> 0 </y>
      <z> 2.0 </z>
    </position>
    <orientation>
      <r>0</r>
      <p>0</p>
      <y>0</y>
    </orientation>

    <xacro:bowtech_stereo_pair/>
    <xacro:range_sensor name="sonar" link="part0" x="-0.3" y="0" z="0" roll="0" pitch="${M_PI/2}" yaw="0" range="10" />
    <xacro:imu_sensor name="imu" link="base_link" x="0" y="0" z="0" roll="0" pitch="0" yaw="${M_PI}" std_dev="0.00000001" />
    <xacro:pressure_sensor name="pressure_sensor" link="base_link" x="0" y="0" z="0.2" roll="0" pitch="0" yaw="0" std_dev="0.02" />
    <xacro:gps_sensor name="gps_sensor" link="base_link" x="0" y="0" z="0.2" roll="0" pitch="0" yaw="0" std_dev="0.00005" />
    <xacro:dvl_sensor name="dvl_sensor" link="base_link" x="-0.75" y="0" z="0.2" roll="0" pitch="0" yaw="3.9269908169872414" std_dev="0.0015" />
    <xacro:depth_camera_sensor name="depth_camera" link="part0" resw="320" resh="240" f="257.986" x="-0.2" y="0.1" z="0" roll="0" pitch="0" yaw="${M_PI/2}"/>

    <multibeamSensor>
      <name>multibeam</name>
      <relativeTo>part0</relativeTo>
      <position>
        <x>-0.2</x>
        <y> 0.1 </y>
        <z> 0 </z>
      </position>
      <orientation>
        <r>0</r>
        <p>0</p>
        <y>3.14 </y>
      </orientation>
      <initAngle>-60</initAngle>
      <finalAngle>60</finalAngle>
      <angleIncr>0.1</angleIncr>
      <range>10</range>
    </multibeamSensor>

    <simulatedDevices>
      <echo>
        <name>g500_echo1</name>
        <info>Echo example 1</info>
      </echo>
    </simulatedDevices>
    <echo>
      <name>g500_echo2</name>
      <info>Echo example 2</info>
    </echo>

    <ForceSensor>
      <name>ForceG500</name>
      <target>base_link</target>
      <offsetp>
        <x>-0.2</x>
        <y>0.75</y>
        <z>0</z>
      </offsetp>
      <offsetr>
        <x>-1.57</x>
        <y>0</y>
        <z>3.14</z>
      </offsetr>
    </ForceSensor>
  </vehicle>

  <object>
    <name>terrain</name>
    <file>terrain/CIRS/cirs_trident.osg</file>
    <position>
      <x> 0</x>
      <y> 0 </y>
      <z> 0 </z>
    </position>
    <orientation>
      <r>0</r>
      <p>0</p>
      <y>0</y>
    </orientation>
    <offsetp>
      <x>-1.5</x>
      <y>-3.0</y>
      <z>0</z>
    </offsetp>
    <offsetr>
      <x> 3.1415</x>
      <y> 0</y>
      <z> -1.57 </z>
    </offsetr>
    <physics>
      <mass> 0 </mass>
      <collisionShapeType> trimesh </collisionShapeType>
    </physics>
  </object>


  <object>
    <name> blackbox </name>
    <file> objects/blackbox_uib_trimesh.osg  </file>
    <position>
      <x> 0</x>
      <y> 0 </y>
      <z> 4.7 </z>
    </position>
    <orientation>
      <r>0</r>
      <p>3.1415</p>
      <y>0</y>
    </orientation>
    <physics>
      <mass> 15 </mass>
      <inertia>
        <x> 0</x>
        <y> 0 </y>
        <z> 0 </z>
      </inertia>
      <collisionShapeType> box </collisionShapeType>
    </physics>
  </object>

  <rosInterfaces>
    <ROSOdomToPAT>
      <topic> /dataNavigator </topic>
      <vehicleName> girona500 </vehicleName>
    </ROSOdomToPAT>
    <WorldToROSTF>
      <rootName> world </rootName>
      <enableObjects> 1 </enableObjects>
      <rate>10</rate>
    </WorldToROSTF>
    <ArmToROSJointState>
	<topic>/uwsim/joint_state</topic>
	<vehicleName> girona500 </vehicleName>
    </ArmToROSJointState>
    <ROSJointStateToArm>
	<topic>/uwsim/joint_state_command</topic>
	<vehicleName> girona500 </vehicleName>
    </ROSJointStateToArm>
    <VirtualCameraToROSImage>
      <cameraName>bowtech1</cameraName>
      <imageTopic> /uwsim/camera1 </imageTopic>
      <infoTopic> /uwsim/camera1_info </infoTopic>
    </VirtualCameraToROSImage>
    <ROSTwistToPAT>
      <topic> /g500/twist</topic>
      <vehicleName> girona500 </vehicleName>
    </ROSTwistToPAT>
    <RangeSensorToROSRange>
      <name>sonar</name>
      <topic> /uwsim/g500/range </topic>
      <rate>10</rate>
    </RangeSensorToROSRange>
    <ROSPoseToPAT>
      <topic> /g500/pose</topic>
      <vehicleName> girona500 </vehicleName>
    </ROSPoseToPAT>
    <ImuToROSImu>
      <name>imu</name>
      <topic>g500/imu</topic>
      <rate>20</rate>
    </ImuToROSImu>
    <PressureSensorToROS>
      <name>pressureSensor</name>
      <topic>g500/pressure</topic>
      <rate>5</rate>
    </PressureSensorToROS>
    <GPSSensorToROS>
      <name>GPSSensor</name>
      <topic>g500/gps</topic>
      <rate>1</rate>
    </GPSSensorToROS>
    <DVLSensorToROS>
      <name>DVLSensor</name>
      <topic>g500/dvl</topic>
      <rate>5</rate>
    </DVLSensorToROS>
    <RangeImageSensorToROSImage>
      <cameraName>rangeImage</cameraName>
      <imageTopic> /uwsim/rangecamera </imageTopic>
      <infoTopic> /uwsim/rangecamera_info </infoTopic>
    </RangeImageSensorToROSImage>
    <multibeamSensorToLaserScan>
      <name>multibeam</name>
      <topic>g500/multibeam</topic>
    </multibeamSensorToLaserScan>
    <SimulatedDeviceROS>
      <echoROS>
        <name>g500_echo1</name>
        <topic>g500/echo1</topic>
        <rate>1</rate>
      </echoROS>
    </SimulatedDeviceROS>
    <contactSensorToROS>
      <name>girona500</name>
      <topic>g500/contactSensor</topic>
      <rate> 100 </rate>
    </contactSensorToROS>
    <echoROS>
      <name>g500_echo2</name>
      <topic>g500/echo2</topic>
      <rate>1</rate>
    </echoROS>
    <ForceSensorROS>
      <name>ForceG500</name>
      <topic>g500/ForceSensor</topic>
      <rate>100</rate>
    </ForceSensorROS>
  </rosInterfaces>
</UWSimScene>
</robot>